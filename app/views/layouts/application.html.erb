<!DOCTYPE html>
<html>
  <head>
    <title>Blog5</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand"><%= link_to "MyBlog",root_path, class: "btn btn-default" %></a>
        </div>  
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to "New Post", new_post_path, class: "btn btn-default" %></li>
        </ul> 
      </div>   
    </nav>        
     <%= yield %>
 </body>
</html>
